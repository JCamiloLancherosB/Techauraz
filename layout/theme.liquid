<noscript><link rel="stylesheet" href="{{ 'home-modern-blocks.css' | asset_url }}"></noscript>

{%- if template.name == 'index' -%}
  <!-- Testimonials - Reviews & carousel section styling (HOME ONLY) -->
  <link rel="preload" href="{{ 'testimonials.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
  <noscript><link rel="stylesheet" href="{{ 'testimonials.css' | asset_url }}"></noscript>
{%- endif -%}

<!-- Custom UI Components - WhatsApp, Announcement Bar, Cookie Banner - 2026-01-21 -->
<link rel="preload" href="{{ 'techauraz-custom-ui.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'techauraz-custom-ui.css' | asset_url }}"></noscript>

{%- if template.name == 'index' -%}
  <!-- Slideshow Enhancements - Controls & CTAs (HOME ONLY) -->
  <link rel="preload" href="{{ 'slideshow-enhancements.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
  <noscript><link rel="stylesheet" href="{{ 'slideshow-enhancements.css' | asset_url }}"></noscript>
{%- endif -%}

<!-- Unified Product Card Styles - Premium card spec for conversion - 2026-01-27 -->
<link rel="preload" href="{{ 'techauraz-product-card.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'techauraz-product-card.css' | asset_url }}"></noscript>

<!-- Utility Classes - Helper classes using design tokens - 2026-01-29 -->
<link rel="preload" href="{{ 'techauraz-utilities.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'techauraz-utilities.css' | asset_url }}"></noscript>

<!-- Announcement Bar Enhancements - Marquee styling with persuasive elements - 2026-01-29 -->
<link rel="preload" href="{{ 'announcement-bar-enhancements.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'announcement-bar-enhancements.css' | asset_url }}"></noscript>

<!-- Header, Announcement Bar & Slideshow UI/UX Fixes - 2026-02-04 -->
<link rel="preload" href="{{ 'header-announcement-slideshow-fixes.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'header-announcement-slideshow-fixes.css' | asset_url }}"></noscript>

{%- if template.name == 'index' or template.name == 'collection' or template.name == 'search' -%}
  <!-- Quick View Modal - Product preview modal styles (PAGES WITH PRODUCT CARDS ONLY) -->
  <link rel="preload" href="{{ 'quick-view-modal.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
  <noscript><link rel="stylesheet" href="{{ 'quick-view-modal.css' | asset_url }}"></noscript>
{%- endif -%}

{%- if template.name == 'product' or template.suffix == 'comparison' -%}
