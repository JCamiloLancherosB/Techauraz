  {%- comment -%} Sticky CTA Bar for Mobile {%- endcomment -%}
  <div class="sticky-cta-bar" id="sticky-cta-bar">
    <div class="sticky-cta-bar__price">
      {{ product.selected_or_first_available_variant.price | money }}
    </div>
    <button class="sticky-cta-bar__button" onclick="document.getElementById('ProductSubmitButton-{{ section.id }}').click();">
      CLICK AQU√ç, PAGA EN CASA üöö
    </button>
  </div>

  <script>
    // Sticky CTA Bar with throttled scroll
    (function() {
      const stickyBar = document.getElementById('sticky-cta-bar');
      const addToCartButton = document.getElementById('ProductSubmitButton-{{ section.id }}');
      
      if (stickyBar && addToCartButton && window.innerWidth < 750) {
        let lastScrollTop = 0;
        let ticking = false;
        
        function checkScroll() {
          const buttonRect = addToCartButton.getBoundingClientRect();
          const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
          
          // Show sticky bar when main button is out of view and scrolling down
          if (buttonRect.bottom < 0 && scrollTop > lastScrollTop) {
            stickyBar.classList.add('visible');
          } else if (buttonRect.top < window.innerHeight) {
            stickyBar.classList.remove('visible');
          }
          
          lastScrollTop = scrollTop;
          ticking = false;
        }
        
        function requestTick() {
          if (!ticking) {
            window.requestAnimationFrame(checkScroll);
            ticking = true;
          }
        }
        
        window.addEventListener('scroll', requestTick, { passive: true });
        checkScroll();
      }
    })();

    // Countdown Timer
    (function() {
      const countdownTimer = document.querySelector('[data-countdown-timer]');
      if (!countdownTimer) return;

      const hoursEl = countdownTimer.querySelector('[data-countdown-hours]');
      const minutesEl = countdownTimer.querySelector('[data-countdown-minutes]');
      const secondsEl = countdownTimer.querySelector('[data-countdown-seconds]');

      // Set countdown to 6 hours from now
      let endTime = new Date().getTime() + (6 * 60 * 60 * 1000);
      
      function updateCountdown() {
        const now = new Date().getTime();
        const distance = endTime - now;

        if (distance < 0) {
          // Reset to 6 hours when countdown ends
          endTime = new Date().getTime() + (6 * 60 * 60 * 1000);
          return;
        }

        const hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
        const minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
        const seconds = Math.floor((distance % (1000 * 60)) / 1000);

        if (hoursEl) hoursEl.textContent = String(hours).padStart(2, '0');
        if (minutesEl) minutesEl.textContent = String(minutes).padStart(2, '0');
        if (secondsEl) secondsEl.textContent = String(seconds).padStart(2, '0');
      }

      updateCountdown();
      setInterval(updateCountdown, 1000);
    })();

    // Update sticky price on variant change
    if (typeof window.ProductInfo !== 'undefined') {
      const originalOnVariantChange = window.ProductInfo.prototype.onVariantChange;
      window.ProductInfo.prototype.onVariantChange = function() {
        if (originalOnVariantChange) originalOnVariantChange.apply(this, arguments);
        
        const stickyPrice = document.querySelector('.sticky-cta-bar__price');
        const currentPrice = document.querySelector('.price-item--regular');
        if (stickyPrice && currentPrice) {
          stickyPrice.textContent = currentPrice.textContent;
        }
      };
    }

    // Hero Slider Dots Navigation
    (function() {
      const slider = document.querySelector('.product-hero-slider');
      if (!slider) return;

      const dots = slider.querySelectorAll('.slider-dot');
      const slides = slider.querySelectorAll('.product-hero-slide');
      
      if (dots.length === 0 || slides.length === 0) return;

      let currentSlide = 0;

      function showSlide(index) {
        // Remove active class from all
        dots.forEach(dot => dot.classList.remove('active'));
        
        // Add active to current
        if (dots[index]) {
          dots[index].classList.add('active');
          currentSlide = index;
        }

        // Use opacity and position for smoother transitions
        if (slides[index]) {
          slides.forEach((slide, i) => {
            if (i === index) {
              slide.style.opacity = '1';
              slide.style.visibility = 'visible';
              slide.style.position = 'relative';
            } else {
              slide.style.opacity = '0';
              slide.style.visibility = 'hidden';
              slide.style.position = 'absolute';
              slide.style.top = '0';
              slide.style.left = '0';
            }
          });
        }
      }

      // Dot click handlers
      dots.forEach((dot, index) => {
        dot.addEventListener('click', () => showSlide(index));
      });

      // Initialize first slide
      showSlide(0);

      // Arrow button handlers
      const prevBtn = slider.querySelector('.slider-prev');
      const nextBtn = slider.querySelector('.slider-next');

      if (prevBtn) {
        prevBtn.addEventListener('click', () => {
          const newIndex = currentSlide > 0 ? currentSlide - 1 : slides.length - 1;
          showSlide(newIndex);
        });
      }

      if (nextBtn) {
        nextBtn.addEventListener('click', () => {
          const newIndex = currentSlide < slides.length - 1 ? currentSlide + 1 : 0;
          showSlide(newIndex);
        });
      }

      // Touch swipe support
      let touchStartX = 0;
      let touchEndX = 0;

      slider.addEventListener('touchstart', (e) => {
        touchStartX = e.changedTouches[0].screenX;
      });

      slider.addEventListener('touchend', (e) => {
        touchEndX = e.changedTouches[0].screenX;
        handleSwipe();
      });

      function handleSwipe() {
        if (touchEndX < touchStartX - 50) {
          // Swipe left - next slide
          const newIndex = currentSlide < slides.length - 1 ? currentSlide + 1 : 0;
          showSlide(newIndex);
        }
        if (touchEndX > touchStartX + 50) {
          // Swipe right - previous slide
          const newIndex = currentSlide > 0 ? currentSlide - 1 : slides.length - 1;
          showSlide(newIndex);
        }
      }
    })();
  </script>
</section>


