              img.style.background = 'linear-gradient(90deg, #f0f0f0 25%, #e0e0e0 50%, #f0f0f0 75%)';
              img.style.backgroundSize = '200% 100%';
              img.style.animation = 'loading 1.5s infinite';
              
              img.addEventListener('load', function() {
                this.style.background = '';
                this.style.animation = '';
              }, { once: true });
            }
            
            observer.unobserve(img);
          }
        });
      }, {
        rootMargin: '50px'
      });

      images.forEach(img => imageObserver.observe(img));
    }
  }

  // 9. An√°liticas de conversi√≥n
  function initConversionAnalytics() {
    const events = {
      pageView: () => {
        if (typeof gtag !== 'undefined') {
          gtag('event', 'page_view', {
            'page_title': document.title,
            'page_location': window.location.href,
            'product_id': '{{ product.id }}',
            'product_name': '{{ product.title | escape }}'
          });
        }
        
        // Tambi√©n enviar a Facebook Pixel si est√° disponible
        if (typeof fbq !== 'undefined') {
          fbq('track', 'ViewContent', {
            content_ids: ['{{ product.id }}'],
            content_type: 'product',
            content_name: '{{ product.title | escape }}',
            value: {{ product.price | divided_by: 100.0 }},
            currency: '{{ cart.currency.iso_code }}'
          });
        }
      },
      
      priceClick: () => {
        if (typeof gtag !== 'undefined') {
          gtag('event', 'price_interaction', {
            'product_id': '{{ product.id }}',
            'interaction_type': 'click'
          });
        }
      },
      
      imageView: () => {
        if (typeof gtag !== 'undefined') {
          gtag('event', 'product_image_view', {
            'product_id': '{{ product.id }}',
            'engagement_level': 'high'
          });
        }
      },
      
      addToCart: () => {
        if (typeof gtag !== 'undefined') {
          gtag('event', 'add_to_cart', {
            'currency': '{{ cart.currency.iso_code }}',
            'value': {{ product.price | divided_by: 100.0 }},
            'items': [{
              'item_id': '{{ product.id }}',
              'item_name': '{{ product.title | escape }}',
              'category': '{{ product.type }}',
              'quantity': 1,
              'price': {{ product.price | divided_by: 100.0 }}
            }]
          });
        }
        
        
        if (typeof fbq !== 'undefined') {
          fbq('track', 'AddToCart', {
            content_ids: ['{{ product.id }}'],
            content_type: 'product',
            value: {{ product.price | divided_by: 100.0 }},
            currency: '{{ cart.currency.iso_code }}'
          });
        }
      }
    };

    // Ejecutar tracking de page view
    setTimeout(events.pageView, 1000);

    // Agregar listeners para otros eventos
    document.querySelectorAll('.price, .product__price').forEach(el => {
      el.addEventListener('click', events.priceClick);
    });

    document.querySelectorAll('.product__media img').forEach(el => {
      el.addEventListener('click', events.imageView);
    });

    document.querySelectorAll('.product-form__cart-submit').forEach(el => {
      el.addEventListener('click', events.addToCart);
    });
  }

  // 10. Inicializar todas las funcionalidades
  function initAll() {
    try {
      // Agregar elementos b√°sicos primero
      addTrustIndicators();
      addUrgencyIndicator();
      
      // Luego inicializar funcionalidades
      setTimeout(() => {
        initUrgencyTimer();
        initUserBehaviorTracking();
        {% comment %} initSocialProof(); {% endcomment %}
        initImageOptimization();
        initConversionAnalytics();
      }, 500);
      
      console.log('‚úÖ Sistema de conversi√≥n inicializado correctamente');
    } catch (error) {
      console.error('‚ùå Error al inicializar sistema de conversi√≥n:', error);
    }
  }

  // Ejecutar inicializaci√≥n
  setTimeout(initAll, 200);

}); // Cierre del DOMContentLoaded
</script>

